{
  "name": "Declaracion Servidores Publicos",
  "version": "1.0.0",
  "private": true,
  "description": "Graphql API",
  "scripts": {
    "build": "rimraf ./build && tsc && copyfiles -u 1 src/**/*.graphql build/ && copyfiles -a -u 1 src/**/*.json build/",
    "lint": "eslint . --ext .ts --cache --fix",
    "start": "npm run build && node build/server.js",
    "start:dev": "nodemon",
    "test": "nyc mocha --recursive --exit --require ts-node/register './src/**/tests/*.test.ts'"
  },
  "license": "UNLICENSED",
  "keywords": [
    "GraphQL",
    "Mongoose"
  ],
  "engines": {
    "node": ">=15.8.0"
  },
  "dependencies": {
    "@graphql-tools/load-files": "~6.2.5",
    "@graphql-tools/merge": "~6.2.7",
    "@graphql-tools/stitch": "~7.3.0",
    "@sendgrid/mail": "^7.4.2",
    "@types/mongoose-paginate-v2": "^1.3.9",
    "apollo-server-express": "~2.21.0",
    "body-parser": "~1.19.0",
    "dotenv": "~8.2.0",
    "express": "~4.17.1",
    "graphql": "~15.5.0",
    "graphql-auth-directives": "~2.2.2",
    "graphql-compose": "^7.25.0",
    "graphql-custom-types": "~1.6.0",
    "jsonwebtoken": "~8.5.1",
    "md5": "~2.3.0",
    "mongodb": "3.6.3",
    "mongoose": "~5.11.16",
    "reflect-metadata": "~0.1.13"
  },
  "devDependencies": {
    "@types/chai": "~4.2.15",
    "@types/express": "~4.17.7",
    "@types/jsonwebtoken": "~8.5.0",
    "@types/md5": "~2.3.0",
    "@types/mocha": "~8.2.0",
    "@typescript-eslint/eslint-plugin": "~4.15.0",
    "@typescript-eslint/parser": "~4.15.0",
    "chai": "~4.3.0",
    "copyfiles": "~2.4.1",
    "eslint": "~7.20.0",
    "graphql-tools-types": "~1.3.1",
    "husky": "~5.0.9",
    "mocha": "~8.3.0",
    "nodemon": "~2.0.7",
    "nyc": "~15.1.0",
    "rimraf": "~3.0.2",
    "ts-node": "~9.1.1",
    "typescript": "~4.1.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run build && npm run test"
    }
  }
}
